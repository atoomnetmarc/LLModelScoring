{
  "require": {
    "guzzlehttp/guzzle": "^7.0",
    "vlucas/phpdotenv": "^5.0",
    "symfony/console": "^7.0",
    "league/csv": "^9.0"
  },
  "require-dev": {
    "pestphp/pest": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "LLMScoring\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LLMScoring\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.4"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
